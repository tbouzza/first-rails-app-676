<h1>Get in touch!</h1>

<form action="/contact" method="get">
  <input type="text"
         name="member"
         value="<%= params[:member] %>"
         placeholder="Who are you looking for?">

  <input type="submit">
</form>

<% if @students.empty? %>
  <p>Not Found</p>
<% end %>

<ul>
  <% @students.each do |student| %>
    <li> <%= student %> </li>
  <% end %>
</ul>
